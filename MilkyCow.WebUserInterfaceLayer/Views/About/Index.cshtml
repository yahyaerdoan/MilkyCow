
@{
    ViewData["Title"] = "About Us";
    ViewData["AboutUsActiveted"] = "active";
    Layout = "~/Views/Layout/Index.cshtml";
}

<!-- Page Header Start -->
@await Component.InvokeAsync("PageHeaderViewComponentPartial", new { title = ViewData["Title"] as string })
<!-- Page Header End -->

<!-- About Start -->
@await Component.InvokeAsync("LayoutAboutViewComponentPartial")
<!-- About End -->

<!-- Banner Start -->
@await Component.InvokeAsync("LayoutBannerViewComponentPartial")
<!-- Banner End -->

<!-- Service Start -->
@await Component.InvokeAsync("LayoutServiceViewComponentPartial")
<!-- Service End -->